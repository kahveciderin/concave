{
  "name": "@kahveciderin/concave",
  "version": "0.5.2",
  "description": "Real-time resource API framework for Express.js with TypeScript client",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./resource": {
      "types": "./dist/resource/hook.d.ts",
      "import": "./dist/resource/hook.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js"
    },
    "./client/react": {
      "types": "./dist/client/react.d.ts",
      "import": "./dist/client/react.js"
    },
    "./tasks": {
      "types": "./dist/tasks/index.d.ts",
      "import": "./dist/tasks/index.js"
    },
    "./kv": {
      "types": "./dist/kv/index.d.ts",
      "import": "./dist/kv/index.js"
    },
    "./oidc": {
      "types": "./dist/oidc/index.d.ts",
      "import": "./dist/oidc/index.js"
    },
    "./openapi": {
      "types": "./dist/openapi/index.d.ts",
      "import": "./dist/openapi/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js"
    },
    "./middleware/observability": {
      "types": "./dist/middleware/observability.d.ts",
      "import": "./dist/middleware/observability.js"
    },
    "./middleware/rateLimit": {
      "types": "./dist/middleware/rateLimit.d.ts",
      "import": "./dist/middleware/rateLimit.js"
    },
    "./middleware/error": {
      "types": "./dist/middleware/error.d.ts",
      "import": "./dist/middleware/error.js"
    },
    "./middleware/logging": {
      "types": "./dist/middleware/logging.d.ts",
      "import": "./dist/middleware/logging.js"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "import": "./dist/env/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "prepublishOnly": "npm run build && npm run test",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "example:dev": "pnpm build && (pnpm example:frontend:build || true) && tsx watch --tsconfig example/tsconfig.json example/src/main.ts",
    "example:server": "pnpm build && tsx watch --tsconfig example/tsconfig.json example/src/main.ts",
    "example:start": "node example/dist/main.js",
    "example:build": "tsc -p example/tsconfig.json && tsc-alias -p example/tsconfig.json",
    "example:frontend:install": "cd example/frontend && pnpm install",
    "example:frontend:build": "cd example/frontend && pnpm build",
    "example:frontend:dev": "cd example/frontend && pnpm dev",
    "example:setup": "pnpm build && pnpm example:frontend:install && pnpm example:frontend:build",
    "example:db:push": "drizzle-kit push",
    "example:typegen": "tsx example/scripts/typegen.ts"
  },
  "keywords": [
    "api",
    "rest",
    "realtime",
    "express",
    "drizzle",
    "orm",
    "crud",
    "typescript",
    "sse",
    "subscriptions",
    "oidc",
    "authentication",
    "background-tasks",
    "offline-first"
  ],
  "author": "kahveciderin",
  "license": "---",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahveciderin/concave.git"
  },
  "homepage": "https://github.com/kahveciderin/concave#readme",
  "bugs": {
    "url": "https://github.com/kahveciderin/concave/issues"
  },
  "dependencies": {
    "@opensearch-project/opensearch": "^3.5.1",
    "cookie-parser": "^1.4.7",
    "cron-parser": "^5.4.0",
    "dotenv": "^17.2.3",
    "drizzle-zod": "^0.8.3",
    "jsonwebtoken": "^9.0.3",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "drizzle-orm": ">=0.30.0",
    "express": ">=4.0.0",
    "react": ">=18.0.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@libsql/client": "^0.17.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.8",
    "@types/react": "^18.3.12",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "better-sqlite3": "^12.6.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9.39.2",
    "express": "^5.2.1",
    "fast-check": "^4.5.3",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.1.4",
    "zod": "^4.3.5"
  }
}